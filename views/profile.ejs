<!--Search-Modal-->

<div class="modal fade" tabindex="-1" role="dialog" data-toggle="modal" data-target="#myModal" id="search">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Search For Listings</h4>
      </div>

      <div class="modal-body">
        <div class="row">
            <div class="col-sm-12">
                <input type="text" class="form-control" id="searchInput" placeholder="LOCATION">
            </div>
        </div>

        

        <br><br>
        <div class="row">
            <div class="col-sm-4">
                <img  alt="Rooms" src="static/images/rooms.png" height="auto" width="50px" id="rooms">
            </div>
            <div class="col-sm-4">
                <img  alt="Pets" src="static/images/pets.png" height="auto" width="45px" id="pets">
            </div>
            <div class="col-sm-4">
                <img  alt="Parking" src="static/images/parking.png" height="auto" width="50px" id="parking">
            </div>
        </div>

        <div class="row">
            <div class="col-sm-4">
                <span class="label label-primary">Rooms</span>  
            </div>
            <div class="col-sm-4">
                <span class="label label-primary">Pet friendly</span>
            </div>
            <div class="col-sm-4">
                <span class="label label-primary">Parking</span>
            </div>
        </div>

        <br><br><br>

        <div class="row">
            <div class="col-sm-4">
                <img  alt="Rooms" src="static/images/tv-internet.png" height="auto" width="50px" id="tv-internet">
            </div>

            <div class="col-sm-4">
                <img  alt="Pets" src="static/images/laundry.png" height="auto" width="50px" id="laundry">
            </div>

            <div class="col-sm-4">
                <img  alt="Parking" src="static/images/sofa.png" height="auto" width="50px" id="sofa">
            </div>
        </div>

         <div class="row">
            <div class="col-sm-4">
                <span class="label label-primary">TV & Internet</span>
            </div>
                <div class="col-sm-4">
                    <span class="label label-primary">Laundry</span>
                </div>
                    <div class="col-sm-4">
                        <span class="label label-primary">Furnished</span>
                    </div>
        </div><br><br>

     </div>

      <div class="modal-footer">
        <i class="fa fa-search fa-3x"></i>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->


<!--Post-Modal-->

<div class="modal fade" tabindex="-1" role="dialog data-toggle="modal" data-target="#myModal" id="post"">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Post A Listing</h4>
      </div>

      <div class="modal-body modal-body-post">

                <div class="row">
                    <div class="col-sm-12">
                         <span class="label label-primary" id="title-label">Title</span><br><br>
                                 <input type="text" class="form-control" placeholder="" aria-describedby="sizing-addon1"> 
                    </div>
                </div>
                <br><br>
                <div class="row">
                    
                        <div class="col-sm-6">
                             <span class="label label-primary" id="number-rooms">Rooms</span><br><br>
                                    <input type="text" class="form-control" placeholder="" aria-describedby="sizing-addon1">   
                        </div>
                    <div class="col-sm-6">
                         <div class="row">
                            <div class="col-sm-6">
                                <span class="label label-primary" id="price-label">Price</span><br><br>
                                <input type="number" class="form-control" placeholder="$" aria-describedby="sizing-addon1"> 
                            </div>
                         </div>
                    </div>                    
                </div>
            
        <br><br>

            
            <div class="row">
                <div class="col-sm-12">
                    <span class="label label-primary" id="details-label">Included :</span><br><br>
                 </div><br>
            </div> 
            <br>    
                    <div class="row1">
